syntax = "v1"

info (
	title:   "参与抽奖"
	desc:    "参与抽奖"
	author:  "jiangxue"
	email:   "896419134@qq.com"
	version: "v1"
)

type LotteryPaticipationReq {
	UserId    int64 `json:"userId"`
	LotteryId int64 `json:"lotteryId"`
}

type LotteryPaticipationResp {
	Id int64 `json:"id"`
}

@server (
	prefix: lottery/v1
	group:  lottery
)
service lottery {
	@doc "lottery participation"
	@handler lotteryPaticipation
	post /lottery/paticipation (LotteryPaticipationReq) returns (LotteryPaticipationResp)
}